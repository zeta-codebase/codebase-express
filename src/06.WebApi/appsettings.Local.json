{
  "Documentation":
  {
    "SwaggerPrefix": "swagger",
    "JsonEndpoint": "/swagger/[version]/swagger.json",
    "Description": "Back-End Services for CodebaseExpress",
    "DescriptionMarkdownFile": "description.md",
    "VersionName": "v1.0"
  },
  "Logging":
  {
    "Using":
    [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel":
    {
      "Default": "Debug",
      "Override":
      {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.Extensions.Http.DefaultHttpClientFactory": "Warning"
      }
    },
    "WriteTo":
    [
      {
        "Name": "Console"
      }
    ]
  },
  "Persistence":
  {
    "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=Local_CodebaseExpressDb;Trusted_Connection=True;MultipleActiveResultSets=true;"
  }
}
