{
  "BackEnd":
  {
    "BaseUrl": "https://localhost:44321"
  },
  "Logging":
  {
    "Using":
    [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel":
    {
      "Default": "Debug",
      "Override":
      {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo":
    [
      { 
        "Name": "Console"
      }
    ]
  },
  "DetailedErrors": true
}
